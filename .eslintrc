{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react"],
  "rules": {
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "comma-dangle": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "react/sort-comp": [
      2,
      {
        "order": [
          "static-methods",
          "lifecycle",
          "everything-else",
          "/^render.+$/",
          "render"
        ]
      }
    ]
  }
}
